{
  "extends": [
    "github>dargmuesli/renovate-config"
  ],
  "docker": {
    "pinDigests": false
  },
  "packageRules": [
    {
      "matchPackageNames": ["maevsi/maevsi"],
      "matchCurrentVersion": "/^.*-alpha/",
      "allowedVersions": "/^.*-alpha/"
    },
    {
      "matchPackageNames": ["maevsi/maevsi"],
      "matchCurrentVersion": "/^.*-beta/",
      "allowedVersions": "/^.*-beta/"
    }
  ]
}